<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Company.
    -->
    <changeSet id="20200216155300-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="bopportunity_id"
                                 baseTableName="company"
                                 constraintName="fk_company_bopportunity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="bopportunity"/>

        <addForeignKeyConstraint baseColumnNames="project_id"
                                 baseTableName="company"
                                 constraintName="fk_company_project_id"
                                 referencedColumnNames="id"
                                 referencedTableName="project"/>

    </changeSet>
</databaseChangeLog>
